msgstr "客户 ID 应该是 18 位数-"

msgid "Scan QR Code to Pay"
msgstr "扫描二维码直接下载Android版工具"

msgid "Payee"
msgstr "收款人"

msgid "Order ID"
msgstr "订单ID"

msgid "Amount"
msgstr "总额"
